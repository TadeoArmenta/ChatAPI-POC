{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "esmodules": true
        }
      }
    ]
  ],
  "plugins": [
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "@src": "./src",
        "@modules": "./src/modules",
        "@database": "./src/database",
        "@utils": ".//src/utils",
      }
    }]
  ]
}
